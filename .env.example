# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_PROJECT_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_PROJECT_ID=your_project_id
SUPABASE_TOKEN=your_token
SUPABASE_API_KEY=your_api_key

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here

# GitHub OAuth Configuration (configured in Supabase Dashboard)
GITHUB_CLIENT_ID=your_github_oauth_client_id
GITHUB_CLIENT_SECRET=your_github_oauth_client_secret

# WireDash Analytics Configuration
# Get your project credentials from https://console.wiredash.com
WIREDASH_PROJECT_ID=your_wiredash_project_id
WIREDASH_SECRET=your_wiredash_secret_key

# In-App Purchase Product IDs
LUCID_MONTHLY_PRODUCT_ID=your_monthly_product_id
LUCID_YEARLY_PRODUCT_ID=your_yearly_product_id
